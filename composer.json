{
	"name": "atomjoy/socialites",
	"description": "Google and Github login buttons with Socialite in Laravel.",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"classmap": [
			"src/"
		],
		"psr-4": {
			"Atomjoy\\Socialites\\": "src/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Migrations\\": "database/migrations/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Atomjoy\\Socialites\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Atomjoy",
			"email": "atomjoy.official@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": "^8.1",
		"laravel/socialite": "^5.14"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Atomjoy\\Socialites\\SocialitesServiceProvider"
			]
		}
	},
	"scripts": {
		"post-socialites-cmd": []
	}
}
